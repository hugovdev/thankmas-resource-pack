{
  "model": {
    "type": "minecraft:select",
    "property": "minecraft:display_context",
    "cases": [
      {
        "when": "gui",
        "model": {
          "type": "minecraft:model",
          "model": "minecraft:item/rods/sungwood_rod_icon"
        }
      }
    ],
    "fallback": {
      "type": "minecraft:condition",
      "property": "minecraft:fishing_rod/cast",
      "on_false": {
        "type": "minecraft:model",
        "model": "minecraft:item/rods/sungwood_rod"
      },
      "on_true": {
        "type": "minecraft:model",
        "model": "minecraft:item/rods/sungwood_rod_cast"
      }
    }
  }
}
